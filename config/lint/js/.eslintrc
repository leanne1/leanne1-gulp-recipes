{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "comma-dangle": 0,
        "indent": 0,
        "strict": [1, "global"],
        "no-var": 0,
        "no-param-reassign": 0,
        "new-cap": [1, {"capIsNewExceptions": ["Map", "List", "Record"]}],
        "no-unused-vars": 0,
        "prefer-const": 0,
        "space-before-blocks": 0,
        "func-names": 0,
        "react/jsx-boolean-value": 0,
        "no-multiple-empty-lines": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
    }
}
